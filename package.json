{
  "name": "react-ags-maps",
  "version": "1.0.14",
  "description": "a hook centric library for creating easy maps with the arcgis api for javascript and react.",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "build": "babel --presets @babel/preset-react src --out-dir dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nick-romano0/react-ags-maps.git"
  },
  "keywords": [
    "map",
    "gis",
    "react",
    "arcgis",
    "javascript",
    "api"
  ],
  "author": "nick-romano",
  "license": "MIT",
  "devDependencies": {
    "@arcgis/core": "^4.20.2",
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/plugin-transform-typescript": "^7.11.0",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@testing-library/react": "^12.0.0",
    "@types/arcgis-js-api": "^4.20.1",
    "@types/jest": "^26.0.24",
    "@types/node": "^15.6.1",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "babel-jest": "^27.0.6",
    "esri-loader": "^2.14.0",
    "jest": "^27.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "typescript": "^4.0.2"
  },
  "peerDependencies": {
    "@arcgis/core": "^4.20.2",
    "esri-loader": "^2.14.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "optionalDependencies": {
    "gl-matrix": "^3.1.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "testMatch": [
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ]
  }
}
